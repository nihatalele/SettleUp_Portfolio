<!-- This partial renders a single trip object. It is used in the index view to display a list of trips. -->
<id="<%= dom_id trip %>">
  <h2>
   <%= trip.name %>
  </h2>
  <p>
    <strong>Start date:</strong>
    <%= trip.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= trip.end_date %>
  </p>
